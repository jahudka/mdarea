{
  "compilerOptions": {
    "lib": ["es2019", "es2020.string", "es2020.symbol.wellknown", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "skipLibCheck": false,
    "sourceMap": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es2019",
    "types": ["node"]
  },
  "include": ["src/**/*"]
}
